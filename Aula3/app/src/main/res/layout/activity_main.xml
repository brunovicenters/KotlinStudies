<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity"
        android:orientation="vertical"
        android:layout_margin="32dp">

    <Button
            android:text="1- Pronome"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btnPronome"
            android:layout_marginTop="16dp"/>

    <Button
            android:text="2 - Preferencia Contato"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btnPrefCon"
            android:layout_marginTop="16dp"/>

    <Button
            android:text="3 - Tipo Tel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btnTipoTel"
            android:layout_marginTop="16dp"/>

    <Button
            android:text="4 - Data e Hora"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btnDataHora"
            android:layout_marginTop="16dp"/>

    <Button
            android:text="5 - Imagem"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btnImagem"
            android:layout_marginTop="16dp"/>

    <Button
            android:text="6 - Quiz"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/btnQuiz"
            android:layout_marginTop="16dp"/>
</LinearLayout>